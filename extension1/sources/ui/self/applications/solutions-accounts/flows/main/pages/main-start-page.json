{
  "title": "",
  "description": "",
  "variables": {
    "accountsListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "site_SolutionsAccounts:crmRest/getall_accounts",
        "keyAttributes": "PartyNumber",
        "itemsPath": "items",
        "responseType": "getallAccountsResponse",
        "filterCriterion": {
          "op": "$co",
          "attribute": "OrganizationName",
          "value": "{{ $variables.searchString }}"
        }
      }
    },
    "oj_table_1606100036_1SelectedId": {
      "type": "string",
      "defaultValue": ""
    },
    "searchString": {
      "type": "string"
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallAccountsResponse": {
      "endpoint": "getall_accounts",
      "service": "site_SolutionsAccounts:crmRest",
      "path": "response"
    },
    "getallAccountsResponse": {
      "items": [
        {
          "CEOName": "string",
          "CurrentFiscalYearPotentialRevenueAmount": "string",
          "NextFiscalYearPotentialRevenueAmount": "string",
          "OrganizationName": "string",
          "PartyNumber": "string"
        }
      ]
    }
  },
  "eventListeners": {
    "accountsDetailButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToAccountsDetailChain",
          "parameters": {
            "accountsId": "[[ $variables.oj_table_1606100036_1SelectedId ]]"
          }
        }
      ]
    },
    "buttonAction": {
      "chains": [
        {
          "chainId": "ButtonActionChain",
          "parameters": {
            "key": "{{ $current.key }}",
            "index": "{{ $current.index }}",
            "current": "{{ $current }}"
          }
        }
      ]
    },
    "editAccountsButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToEditAccountsChain",
          "parameters": {
            "accountsId": "[[ $variables.oj_table_1606100036_1SelectedId ]]"
          }
        }
      ]
    },
    "oj_table_1606100036_1SelectionChanged": {
      "chains": [
        {
          "chainId": "oj_table_1606100036_1ChangeSelectionChain",
          "parameters": {
            "accountsId": "[[ $event.detail.value.key || '' ]]"
          }
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-input-search": {
        "path": "ojs/ojinputsearch"
      },
      "oj-sp-collection-container": {
        "path": "oj-sp/collection-container/loader"
      },
      "oj-sp-welcome-page": {
        "path": "oj-sp/welcome-page/loader"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      }
    }
  }
}
